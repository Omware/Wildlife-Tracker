{{#partial "content"}}

<div class="row">
    <div class="col-md-6">
        <h3>Not Endagered</h3>

        <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Type</th>

        </tr>
        {{#each Nanimal}}
            <tr>
                <td>{{id}}</td>
                <td>{{name}}</td>
                <td>{{type}}</td>

            </tr>
        {{/each}}

    </table>
</div>
<div class="col-md-6">
    <h3>Endagered</h3>
    <table>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Type</th>
            <th>Health</th>
            <th>Age</th>
        </tr>
        {{#each Eanimal}}
            <tr>
                <td>{{id}}</td>
                <td>{{name}}</td>
                <td>{{type}}</td>
                <td>{{health}}</td>
                <td>{{age}}</td>

            </tr>
        {{/each}}

    </table>
</div>
</div>
{{/partial}}

{{> layout.hbs}}